    "eslint": "^6.1.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.0",
