    "babel-eslint": "8.2.6",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "deep-freeze": "^0.0.1",
    "electron": "^10.1.5",
